 "doctype": "Print Format",
 "font_size": 14,
 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n  <head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <meta http-equiv=\"X-UA-Compatible\" content=\"ie=edge\">\n  </head>\n  <body>\n   \n   <div class = \"min-width\" id = \"Entire_Form\">\n       \n        <div id = \"First_Section\" class = \"border\">\n           <h2 class = \"margin-0\"><b class = \"double font-size-18px\">DELIVERY CHALLAN</b></h2>\n           <span class = \"border inline-block left font-size-15px\" style=\"bottom: 15px; width:250px\">\n               {% set formatted_date = frappe.utils.formatdate(doc.posting_date, \"dd / MM / yyyy\") %}\n           <p class = \"underline margin-0\" style = \"padding:0 0 0 2px\">Date: {{formatted_date}}</p>\n           <p class = \"margin-0\" style = \"padding:0 0 0 2px\">Challan No: {{doc.name}}</p>\n           </span>\n       </div>\n        <div id = \"Second_Section\" class = \"flex border top-border-0\" style=\"border-bottom:0\">\n           <div class = \"width-10 aligning-center\">\n               <p class = \"\" style=\"\"><b class = \"font-size-15px\">TO :</b></p>\n           </div>\n           <div class = \"padding-5px\">\n               <h2 class=\"margin-0\"><b class = \"font-size-18px\">{{doc.customer}}</b></h2>\n               <p class = \"font-size-15px\">\n                   {%if doc.customer_address%}\n                   {% set customer_address_data = frappe.get_doc(\"Address\",doc.customer_address)%}\n                   <p class = \"font-size-15px\">{{customer_address_data.address_line1}}{{customer_address_data.address_line2}}{{customer_address_data.city}},{{customer_address_data.state}} - {{customer_address_data.pincode}}</p>\n                   {%endif%}\n               </p>\n           </div>\n           \n       </div>\n        <div id = \"Third_Section\"  class = \"border\">\n           <div class = \"\">\n               <p class = \"\">\n                   <b class = \"font-size-15px margin-0\" style=\"padding-left:2px\">Delivered By: M/s. {{doc.company}}</b>\n                </p>\n           </div>\n       </div>\n        <div id = \"Fourth_Section\" class = \"border top-border-0\"><br></div> \n        <div id = \"Fifth_Section\" class = \"flex border top-border-0\" style=\"border-bottom:0\">\n            <div class = \"width-15\">\n            <p class = \"\" style=\"padding-left:6px\"><b  class = \"font-size-15px aligning-center\" style=\"justify-content:flex-start\">Your Order No:</b></p>    \n            </div>\n            <div class = \"aligning-center\">\n                {% set formatted_date = frappe.utils.formatdate(doc.po_date, \"dd / MM / yyyy\") %}\n                <p><b class = \"font-size-15px\" style=\"padding-left:3px\">PO No: {{doc.po_no}} &nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Date: {{formatted_date}} </b></p>\n            </div>\n         </div>\n        <div id = \"Sixth_Section\" class = \"border top-border-0\" >\n            <div class = \"min-width\">\n            {% set item_count = doc.items | length %}\n           <table class=\"min-width\">\n            <tr class=\"\" style=\"border-bottom: 3px solid black; border-inline-width: 2px;\">\n                <th class=\"tableheaderborder font-size-15px\" style=\"width:10%; text-align:center; border-right:0; border-left:0;\">Sr. No.</th>\n                <th class=\"tableheaderborder font-size-15px\" style=\"width:45%; text-align:center;\">Description</th>\n                <th class=\"tableheaderborder font-size-15px\" style=\"width:15%; text-align:center;\">Quantity</th>\n                <th class=\"tableheaderborder font-size-15px\" style=\"width:30%; text-align:center; border-left:2px solid black;\">Remarks</th>\n            </tr>\n            <tr style=\"height:20px\">\n            <td class=\"tableborder font-size-15px\" style=\"text-align:center;\"></td>\n            <td class=\"tableborder font-size-15px\" style=\"text-align:center;\"></td>\n            <td class=\"tableborder font-size-15px\" style=\"text-align:center;\"></td>\n            <td class=\"tableborder font-size-15px\" style=\"border-left:2px solid black; border-bottom-color:white;border-top-color:white;border-right-color:white;border-right:0\"></td>\n            </tr>\n            <!-- Loop through items and generate rows -->\n            {% for item in doc.items %}\n            <tr class=\"tableborder\">\n                <td class=\"tableborder font-size-15px\" style=\"text-align:center;border-left:0\">{{ loop.index }}</td>\n                <td class=\"tableborder font-size-15px\">{{ item.description }}</td>\n                <td class=\"tableborder font-size-15px\" style=\"text-align:center; border-right: 2px solid black;\">{{ item.qty }} {{ item.uom }}</td>\n        \n                <!-- Apply rowspan for Remarks in the first row only -->\n                {% if loop.first %}\n                <td class=\"tableborder\" rowspan=\"{{ doc.items | length }}\" style=\"border-left: 2px solid black; text-align:center; border-bottom-color:white; border-top-color:white;border-right:0\"></td>\n                {% endif %}\n            </tr>\n        \n            <!-- Spacer row after each iteration -->\n            {% if not loop.last %}\n            <tr style=\"height:20px\">\n                <td class=\"tableborder font-size-15px\" style=\"text-align:center;\"></td>\n                <td class=\"tableborder font-size-15px\" style=\"text-align:center;\"></td>\n                <td class=\"tableborder font-size-15px\" style=\"text-align:center; border-right: 2px solid black;\"></td>\n                <td class=\"tableborder font-size-15px\" style=\"text-align:center;  border-bottom-color:white; border-top-color:white;border-right-color:white;border-right:0\"></td>\n            </tr>\n            {% endif %}\n            {% endfor %}\n            \n            <tr style=\"height:20px\">\n                <td class=\"tableborder font-size-15px\" style=\"text-align:center;\"></td>\n                <td class=\"tableborder font-size-15px\" style=\"text-align:center;\"></td>\n                <td class=\"tableborder font-size-15px\" style=\"text-align:center;\"></td>\n                <td class=\"tableborder font-size-15px\" style=\"border-left:2px solid black; border-bottom-color:white;border-top-color:white;border-right:0\"></td>\n            </tr>\n        </table>\n\n\n            </div>\n        </div> \n        <div id = \"Last_Section\" class = \"border top-border-0\">\n            <div class = \"flex font-size-15px\">\n                <div style = \"min-width:40%\">\n                <p style=\"\">This receipt form should be signed by the person having authority and returned to the bearer</p>\n                </div>\n                <div style=\"min-width:20\"></div>\n                <div style = \"min-width:40%\">\n                <p style = \"text-align: end;\"><b class = \"font-size-15px\">Received the above goods in order</b></p><p style=\"text-align: end;\"><b class = \"font-size-15px\" style=\"\">and condition</b></p>    \n                </div>\n            </div>\n            <br><br><br>\n            <div class = \"flex font-size-15px\">\n                <div style = \"min-width:40%\">\n                    <p style=\"\">No complaints will be entertained if the same are received after 24 hours after delivery</p>\n                </div>\n                 <div style=\"min-width:20\"></div>\n                <div style = \"min-width:40%\">\n                    <p style=\"text-align: end;\"><b>Receiver's Signature with Stamp</b></p>    \n                </div>\n            </div>\n        </div> \n         \n   </div>\n   \n   \n  </body>\n</html>",
