 "css": "* {\n    margin:0;\n    padding:0;\n    font-size:15px;\n}\n\n.border{\n    border: 1px solid black;\n    border-inline-width: 1px;\n}\n\n.tableborder{\n    border:1px solid gray;\n    border-inline-width: 1px;\n    border-left:0;\n}\n.tableheaderborder{\n    border: 1px solid black;\n    border-inline-width: 1px;\n    border-right:0;\n    \n    border-bottom:0;\n}\n.top-border-0{\n border-top:0;   \n}\n\n.border-right-color{\n     border-right-color: black;\n}\n\n.double{\n    border-bottom: 4px double black;\n    line-spacing:10px;\n    margin-top:2px;\n}\n\n.underline{\n    border-bottom: 1px solid black;\n    margin:0;\n}\n\n.min-width{\n    min-width:100% !important;\n}\n\n.width-10{\n    min-width:10%;\n    border-right:1px solid black;\n    border-inline-width: 1px;\n}\n\n.width-15{\n    min-width:15%;\n    border-right:1px solid black;\n}\n\n.border-left{\n    border-left:2px solid black;\n}\n\n.margin-0{\n    margin:0;\n}\n\n.inline-block{\n    display:inline-block;\n}\n\n.left{\n    position: relative;\n    left:55%;\n}\n\n.margin-3px{\n    margin:3px;\n}\n\n.flex{\n    display:flex;\n}\n\n.aligning-center{\n    justify-content: center;\n    display: flex;\n    align-items: center;\n}\n\n.padding-5px{\n    padding:5px;\n}\n\n.font-size-18px{\n    font-size:18px;\n}\n\n.font-size-15px{\n    font-size:16px;\n}\n\n.print-format td, .print-format th{\n    padding:0 !important;\n    margin:0 !important;\n}",
